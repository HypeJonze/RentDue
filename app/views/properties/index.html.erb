<h1>Listing properties</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>
  

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%=property.address %></td>
        <td><%=property.province%></td>
        <td><%=property.city%></td>
        <td><%=property.postcode %></td>
        <td><%=property.rent%></td>
        <td><%=property.utilies%></td>
        <td><%=link_to "Show Property",user_property_path(@user,property) , class: "btn-default btn"%></td>
        <td><%= link_to "Edit Property", edit_user_property_path(@user,property), class: "btn-default btn"%></td>
        
       
      </tr>

    <% end %>
    <td> <%=link_to 'User main page',user_path(@user ), class: "btn-default btn"%></td>
  </tbody>
</table>

<br>
