<h1>Listing properties</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>
  

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%=property.city %></td>
        <td><%=property.province%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
